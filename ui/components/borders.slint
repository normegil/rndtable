import { Palette } from "../palette.slint";

export component BorderVertical inherits Rectangle {
    in-out property <length> size: 1px;

    y: 0;
    width <=> size;
    background: Palette.borders;
}

export component BorderHorizontal inherits Rectangle {
    y: 0;
    height: 1px;
    background: Palette.borders;
}